#include<iostream>
using namespace std;
void swap(char& a, char& b)
{
	char tmp =a;
	a = b;
	b = tmp;
}



//将字符串前5个尾插到末尾
int main100()
{
	char s[] = { "abxsxcdef" };
	int size = sizeof(s) - 2;//计算时注意末尾的 '\0'
	for (int i = 0; i < (5/2); i++)
	{
		swap(s[i], s[4 - i]);
	}
	for (int i = 5; i < size  ; i+=2)
	{
		swap(s[i], s[size + 5 - i]);
	}
	cout << s<<endl;
	for (int i = 0; i < size / 2; i++)
	{
		swap(s[i], s[size - i]);
	}
	cout << s << endl;
	return 0;
}

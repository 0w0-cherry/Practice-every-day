int main()
{
	int n = 1020;
	int count = 0;
	while (n)
	{
		int a = n / 2 + 2;
		n -= a;
		count++;
	}
	cout << count << "天才能卖完";
	return 0;
}

int main()
{
	int i;
	cin >> i;
	if (i == 6 || i == 7)
	{
		if (i == 6)
			cout << "去长城";
		else
			cout << "去故宫";
	}
	else
	{
		if (i == 1)
			cout << "开会";
		else
		{
			cout << "正常工作";
		}
	}
	return 0;
}

int main()
{
	double length = 4.2, wide = 1.9, high = 1.9;
	int a = length / 0.5;
	int b = wide / 0.5;
	int c =high/ 0.3;
	cout << a * b * b << endl;
	return 0;
}

